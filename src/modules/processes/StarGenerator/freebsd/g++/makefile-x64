#

SRC_FILES= \
../../Projection.cpp \
../../StarGeneratorInstance.cpp \
../../StarGeneratorInterface.cpp \
../../StarGeneratorModule.cpp \
../../StarGeneratorParameters.cpp \
../../StarGeneratorProcess.cpp

#
# Object files
