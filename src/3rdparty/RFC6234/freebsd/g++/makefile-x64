-include $(DEP_FILES)

$(OBJ_DIR)/libRFC6234-pxi.a: $(OBJ_FILES)
	ar r $(OBJ_DIR)/libRFC6234-pxi.a $(OBJ_FILES)
	$(MAKE) -f ./makefile-x64 --no-print-directory post-build

.PHONY: clean
clean:
	rm -f $(OBJ_FILES) $(DEP_FILES) $(OBJ_DIR)/libRFC6234-pxi.a

.PHONY: post-build
post-build:
	cp $(OBJ_DIR)/libRFC6234-pxi.a $(PCLLIBDIR64)

./x64/Release/%.o: ../../%.c
	clang -c -pipe -pthread -m64 -fPIC -D_REENTRANT -D__PCL_FREEBSD -I"$(PCLINCDIR)" -I"$(PCLSRCDIR)/3rdparty" -mtune=corei7 -msse3 -minline-all-stringops -O3 -fomit-frame-pointer -ffunction-sections -fdata-sections -ffast-math -std=c99 -Wall -Wno-parentheses -MMD -MP -MF"$(@:%.o=%.d)" -o"$@" "$<"
	@echo ' '

