OBJ_FILES= \
./x64/Release/Projection.o \
./x64/Release/StarGeneratorInstance.o \
./x64/Release/StarGeneratorInterface.o \
./x64/Release/StarGeneratorModule.o \
./x64/Release/StarGeneratorParameters.o \
