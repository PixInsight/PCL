OBJ_DIR="$(PCLSRCDIR)/installer/x11/freebsd/g++/x64/Release"

.PHONY: all
all: $(OBJ_DIR)/installer

#
# Source files
#

SRC_FILES= \
../../installer.cpp

#
# Object files
#

OBJ_FILES= \
./x64/Release/installer.o

#
# Dependency files
#

DEP_FILES= \
./x64/Release/installer.d

#
# Rules
#

-include $(DEP_FILES)

$(OBJ_DIR)/installer: $(OBJ_FILES)
	clang++ -m64 -fPIC -Wl,-z,noexecstack -Wl,-O1 -Wl,--gc-sections -s -L"$(PCLLIBDIR64)" -L"$(PCLBINDIR64)" -o $(OBJ_DIR)/installer $(OBJ_FILES) -lpthread -lPCL-pxi -llz4-pxi -lzlib-pxi -lRFC6234-pxi -llcms-pxi -lcminpack-pxi
	$(MAKE) -f ./makefile-x64 --no-print-directory post-build

.PHONY: clean
clean:
	rm -f $(OBJ_FILES) $(DEP_FILES) $(OBJ_DIR)/installer

.PHONY: post-build
post-build:
	cp $(OBJ_DIR)/installer $(PCLBINDIR64)/../..

./x64/Release/%.o: ../../%.cpp
	clang++ -c -pipe -pthread -m64 -fPIC -D_REENTRANT -D__PCL_FREEBSD -I"$(PCLINCDIR)" -I"$(PCLSRCDIR)/3rdparty" -mtune=corei7 -msse3 -minline-all-stringops -Os -fomit-frame-pointer -ffunction-sections -fdata-sections -std=c++11 -Wall -Wno-parentheses -Wno-extern-c-compat -MMD -MP -MF"$(@:%.o=%.d)" -o"$@" "$<"
	@echo ' '

