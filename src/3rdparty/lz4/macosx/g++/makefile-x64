-include $(DEP_FILES)

$(OBJ_DIR)/liblz4-pxi.a: $(OBJ_FILES)
	ar r $(OBJ_DIR)/liblz4-pxi.a $(OBJ_FILES)
	$(MAKE) -f ./makefile-x64 --no-print-directory post-build

.PHONY: clean
clean:
	rm -f $(OBJ_FILES) $(DEP_FILES) $(OBJ_DIR)/liblz4-pxi.a

.PHONY: post-build
post-build:
	cp $(OBJ_DIR)/liblz4-pxi.a $(PCLLIBDIR64)

./x64/Release/%.o: ../../%.c
	clang -c -pipe -pthread -arch x86_64 -fPIC -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX10.12.sdk -mmacosx-version-min=10.10 -D_REENTRANT -D__PCL_MACOSX -I"$(PCLINCDIR)" -I"$(PCLSRCDIR)/3rdparty" -mtune=corei7 -mssse3 -minline-all-stringops -O3 -ffunction-sections -fdata-sections -ffast-math -std=c99 -Wall -Wno-parentheses -MMD -MP -MF"$(@:%.o=%.d)" -o"$@" "$<"
	@echo ' '

