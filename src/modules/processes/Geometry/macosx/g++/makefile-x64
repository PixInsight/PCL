.PHONY: clean
clean:
	rm -f $(OBJ_FILES) $(DEP_FILES) $(OBJ_DIR)/Geometry-pxm.dylib

.PHONY: post-build
post-build:
	cp $(OBJ_DIR)/Geometry-pxm.dylib $(PCLBINDIR64)
	codesign --deep -s pleiades -f -v --timestamp $(PCLBINDIR64)/Geometry-pxm.dylib

