$(OBJ_DIR)/Debayer-pxm.dylib: $(OBJ_FILES)
	clang++ -arch x86_64 -fPIC -headerpad_max_install_names -Wl,-syslibroot,/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX10.12.sdk -mmacosx-version-min=10.11 -stdlib=libc++ -Wl,-dead_strip -dynamiclib -install_name @executable_path/Debayer-pxm.dylib -L"$(PCLLIBDIR64)" -L"$(PCLBINDIR64)" -o $(OBJ_DIR)/Debayer-pxm.dylib $(OBJ_FILES) -framework CoreFoundation -lpthread -lPCL-pxi -llz4-pxi -lzlib-pxi -lRFC6234-pxi -llcms-pxi -lcminpack-pxi
	$(MAKE) -f ./makefile-x64 --no-print-directory post-build

.PHONY: clean
clean:
	rm -f $(OBJ_FILES) $(DEP_FILES) $(OBJ_DIR)/Debayer-pxm.dylib

.PHONY: post-build
post-build:
	cp $(OBJ_DIR)/Debayer-pxm.dylib $(PCLBINDIR64)
	codesign --deep -s pleiades -f -v --timestamp $(PCLBINDIR64)/Debayer-pxm.dylib

