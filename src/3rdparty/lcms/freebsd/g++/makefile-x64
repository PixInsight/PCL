$(OBJ_DIR)/liblcms-pxi.a: $(OBJ_FILES)
	ar r $(OBJ_DIR)/liblcms-pxi.a $(OBJ_FILES)
	$(MAKE) -f ./makefile-x64 --no-print-directory post-build

.PHONY: clean
clean:
	rm -f $(OBJ_FILES) $(DEP_FILES) $(OBJ_DIR)/liblcms-pxi.a

.PHONY: post-build
post-build:
	cp $(OBJ_DIR)/liblcms-pxi.a $(PCLLIBDIR64)

./x64/Release/%.o: ../../%.c
