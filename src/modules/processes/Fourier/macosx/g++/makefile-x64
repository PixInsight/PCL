.PHONY: post-build
post-build:
	cp $(OBJ_DIR)/Fourier-pxm.dylib $(PCLBINDIR64)
	codesign --deep -s pleiades -f -v --timestamp $(PCLBINDIR64)/Fourier-pxm.dylib

