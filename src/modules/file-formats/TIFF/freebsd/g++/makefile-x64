.PHONY: clean
clean:
	rm -f $(OBJ_FILES) $(DEP_FILES) $(OBJ_DIR)/TIFF-pxm.so

.PHONY: post-build
post-build:
	cp $(OBJ_DIR)/TIFF-pxm.so $(PCLBINDIR64)

./x64/Release/%.o: ../../%.cpp
